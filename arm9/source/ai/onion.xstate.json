{
  "id": "onion",
  "initial": "idle",
  "states": {
    "idle": {
      "on": {
        "SEEDS_INCREASED": {
          "target": "reproducing",
          "actions": "updateSeedCounter"
        }
      }
    },
    "reproducing": {
      "initial": "bounce",
      "on": {
        "SEEDS_INCREASED": {
          "target": "reproducing",
          "actions": "updateSeedCounter"
        }
      },
      "states": {
        "bounce": {
          "on": {
            "LAST_FRAME": "windUp"
          }
        },
        "windUp": {
          "on": {
            "LAST_FRAME": "ejectSeeds"
          }
        },
        "ejectSeeds": {
          "on": {
            "NO_MORE_SEEDS": "windDown"
          },
          "after": {
            "667": {
              "target": "ejectSeeds",
              "actions": "ejectSeeds"
            }
          }
        },
        "windDown": {
          "on": {
            "LAST_FRAME": "done"
          }
        },
        "done": {
          "type": "final"
        }
      },
      "onDone": "idle"
    }
  }
}