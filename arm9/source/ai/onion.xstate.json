{
  "id": "onion",
  "initial": "idle",
  "states": {
    "idle": {
      "on": {
        "SEEDS_INCREASED": {
          "target": "bounce",
          "actions": "updateSeedCounter"
        }
      }
    },
    "bounce": {
      "on": {
        "SEEDS_INCREASED": {
          "target": "bounce",
          "actions": "updateSeedCounter"
        }
      }
    },
    "windUp": {
      "on": {
        "SEEDS_INCREASED": {
          "target": "bounce",
          "actions": "updateSeedCounter"
        }
      }
    },
    "windDown": {
      "on": {
        "SEEDS_INCREASED": {
          "target": "bounce",
          "actions": "updateSeedCounter"
        },
        "LAST_FRAME": "idle"
      }
    },
    "ejectSeeds": {
      "on": {
        "SEEDS_INCREASED": {
          "target": "bounce",
          "actions": "updateSeedCounter"
        },
        "NO_MORE_SEEDS": "windDown"
      },
      "after": {
        "667": {
          "target": "ejectSeeds",
          "actions": "ejectSeeds"
        }
      }
    }
  }
}