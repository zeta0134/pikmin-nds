{
  "id": "onion",
  "initial": "idle",
  "states": {
    "idle": {
      "on": {
        "SEEDS_INCREASED": {
          "target": "reproducing",
          "actions": "updateSeedCounter"
        }
      }
    },
    "reproducing": {
      "initial": "bounce",
      "on": {
        "SEEDS_INCREASED": {
          "target": "reproducing",
          "actions": "updateSeedCounter"
        }
      },
      "states": {
        "bounce": {
          "on": {
            "LAST_FRAME": "windUp"
          }
        },
        "windUp": {
          "on": {
            "LAST_FRAME": {
              "target": "moreSeedsLeft",
              "actions": "ejectSeeds"
            }
          }
        },
        "ejectSeeds": {
          "after": {
            "667": {
              "target": "moreSeedsLeft",
              "actions": "ejectSeeds"
            }
          }
        },
        "moreSeedsLeft": {
          "on": {
            "": [
              {
                "target": "ejectSeeds",
                "cond": "moreSeedsLeft"
              },
              {
                "target": "windDown"
              }
            ]
          }
        },
        "windDown": {
          "on": {
            "LAST_FRAME": "done"
          }
        },
        "done": {
          "type": "final"
        }
      },
      "onDone": "idle"
    }
  }
}